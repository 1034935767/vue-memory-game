{
  "name": "vue-memory-game",
  "version": "2.0.0",
  "description": "A tiny game written in vue. It's inspired by IgorMinar's Memory-Game",
  "scripts": {
    "start": "webpack-dev-server --content-base build/ --hot --host 0.0.0.0",
    "dist": "rm -rf dist; webpack --config webpack.config.prod.js",
    "deploy": "npm run dist; git checkout gh-pages; rm -rf *.png; rm -rf *.js; rm -rf *.ico; rm -rf index.html; mv dist/* .;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/vue-memory-game.git"
  },
  "keywords": [
    "memory-game",
    "vue",
    "component"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/vue-memory-game/issues"
  },
  "homepage": "https://github.com/leftstick/vue-memory-game#readme",
  "dependencies": {
    "vue": "^2.0.5",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.8.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
